<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>DHTMLX</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>dhtmx, scheduler, recurring, recurring events, asp.net core, asp.net</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://docs.dhtmlx.com/scheduler/</PackageProjectUrl>
    <Version>1.0.0</Version>
    <RepositoryUrl>https://github.com/DHTMLX/scheduler-recurring-events-dotnet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>A helper library for parsing recurring events of [dhtmlxScheduler](https://docs.dhtmlx.com/scheduler/) on ASP.NET/ASP.NET Core backends.</Description>
  </PropertyGroup>

</Project>
